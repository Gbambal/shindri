    <bpmn2:task id="_69F8F54A-14BB-4C81-A54C-2522831A5475" drools:taskName="ExecuteSQL" name="Get DB data for trip">
      <bpmn2:documentation id="_uQer9P4VEe6aio2ydXOwfA"><![CDATA[execute-sql-workitem/index.html]]></bpmn2:documentation>
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[Get DB data for trip]]></drools:metaValue>
        </drools:metaData>
        <drools:onEntry-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[System.out.println("request tagId" + tagId);

String getTripQuery = "SELECT mv.*,t.tareWeight,t.tw,cd.challanQty,mv.material_code as allowed_material,mm.code as actual_material,cd.di_qty FROM provisionalPlms.vw_trip_stages mv Inner join provisionalPlms.trip t on mv.tripid=t.tripid inner join provisionalPlms.vw_consignmentdetails cd on cd.tripId=t.tripId left join provisionalPlms.materialTypesMaster mtm on mtm.code = t.materialtype_code left join provisionalPlms.materialMaster mm on mm.materialtypes_code=mtm.code	where mv.tag_id ='" + tagId + "' and mv.movementCode = t.movementCode and mv.curr_location = t.mapPlantStageLocation and mv.di_number IS NOT NULL Limit 1";
kcontext.setVariable("getTripdbQuery",getTripQuery);
System.out.println("------executing get db data-------"+getTripQuery);
]]></drools:script>
        </drools:onEntry-script>
        <drools:onExit-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[try {
Object dbTripDataResult = kcontext.getVariable("getTripdbQueryResp");
System.out.println("------executing get db data-------"+dbTripDataResult);

		if(dbTripDataResult != null){
		if (dbTripDataResult instanceof ArrayList<?>) {
			ArrayList<?> dbresultList = (ArrayList<?>) dbTripDataResult;
			for (Object dbelements : dbresultList) {
				String[] dbcolumn = dbelements.toString().split(",");
				
				String dbtripId = dbcolumn[0];
				System.out.println("--------------view: tripId--" + dbtripId);
				kcontext.setVariable("tripId",dbtripId);
				
				String dbvehicleNumber = dbcolumn[1];
				System.out.println("--------------view: vehicleNo--" + dbvehicleNumber);
				kcontext.setVariable("vehicleNo",dbvehicleNumber);
				
				String dbplantCode = dbcolumn[13];
				System.out.println("--------------view: plantCode--" + dbplantCode);
				kcontext.setVariable("plantCode",dbplantCode);
				
				String dbcompanyCode = dbcolumn[14];
				System.out.println("--------------view: companyCode--" + dbcompanyCode);
				kcontext.setVariable("companyCode",dbcompanyCode);
            				
				String dbnextLoc = dbcolumn[7];
				System.out.println("--------------view: nextLoc--" + dbnextLoc);
				kcontext.setVariable("nextlocation",dbnextLoc);
								
				String dbcurrentSeq = dbcolumn[11];
				System.out.println("--------------view: currentSeq--" + dbcurrentSeq);
				kcontext.setVariable("currentSeq",dbcurrentSeq);
				
				int number  = Integer.parseInt(dbcurrentSeq);

				 // Increment the number
				number++;

				// Convert the incremented number back to a string
				String incrementedSeq = Integer.toString(number);

				System.out.println("Original Sequence: " + dbcurrentSeq);
				System.out.println("Incremented Sequence: " + incrementedSeq);
				kcontext.setVariable("updatedSeq",incrementedSeq);

					
			}
		} else {
			System.out.println("Tag ID not found");

		}
		}else{
			System.out.println("No data for Tag ID");
		}
		}catch (Exception e) {
		System.out.println("Exception in getDB data service: " + e.getMessage());
}
	System.out.println("Get DB data completed successfully");]]></drools:script>
        </drools:onExit-script>
