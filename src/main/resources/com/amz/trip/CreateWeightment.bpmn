        <drools:onEntry-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[org.json.JSONObject jsonObject = new org.json.JSONObject();

// Add properties with dynamic values
jsonObject.put("tripId", tripId);
jsonObject.put("vehicleNumber", vehicleNo);
jsonObject.put("plantCode", plantCode);
jsonObject.put("companyCode", companyCode);
jsonObject.put("status", "A");
jsonObject.put("mapPlantStageLocation", nextlocation);

if (nextlocation.contains("TW")) {
    jsonObject.put("tw", weight);
} else if (nextlocation.contains("GW")) {
    jsonObject.put("gw", weight);
}

String incrementedSeq = String.valueOf(Integer.parseInt(currentSeq) + 1);
jsonObject.put("sequence_number", incrementedSeq);

// Define a variable to hold the updated time
LocalDateTime currentTime = LocalDateTime.now();

// Define a formatter for LocalDateTime
DateTimeFormatter formatter = DateTimeFormatter.ofPattern("yyyy-MM-dd'T'HH:mm:ss");

// Add timestamp based on location
String formattedTime = currentTime.format(formatter);
if (nextlocation.contains("TW")) {
    jsonObject.put("tareWeight", formattedTime);
} else if (nextlocation.contains("GW")) {
    jsonObject.put("grossWeight", formattedTime);
}

// Convert JSONObject to a string
String jsonString = jsonObject.toString();
System.out.println("--json request is:" + jsonString);
kcontext.setVariable("restJson", jsonString);
]]></drools:script>
        </drools:onEntry-script>
        <drools:onExit-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[System.out.println("--- response obj is: " + resp);

String jsonString = resp;

// Use Jackson ObjectMapper to parse JSON
ObjectMapper objectMapper = new ObjectMapper();
try {
    // Parse JSON string to JsonNode
    JsonNode jsonNode = objectMapper.readTree(jsonString);

    // Check if the "message" field exists
    JsonNode messageNode = jsonNode.get("message");
    if (messageNode != null && messageNode.isTextual()) {
        // Extract the "message" field
        String messageValue = messageNode.asText();

        // Print the extracted value
        System.out.println("Message Value: " + messageValue);
        kcontext.setVariable("centralMsgresp", messageValue);
    } else {
        System.out.println("No 'message' field found in the JSON response.");
        // Handle the absence of the "message" field accordingly
    }
} catch (Exception e) {
    e.printStackTrace();
}

kcontext.setVariable("status","SUCCESS");]]></drools:script>
        </drools:onExit-script>
