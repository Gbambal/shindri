      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[create weightment]]></drools:metaValue>
        </drools:metaData>
        <drools:onEntry-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[StringBuilder queryBuilder = new StringBuilder();

if (weightType.equals("TW")) {
    queryBuilder.append("INSERT INTO provisionalPlms.weightmentDetails (tripId, weightType, weight, entryTime, exitTime, weighbridgeCode, weighridgeImg) VALUES ('")
                .append(tripId).append("','").append(weightType).append("', '").append(weight).append("','").append(entryTime).append("','").append(exitTime)
                .append("','").append(weighbridgeCode).append("','").append(weighridgeImg).append("')");
} else {
    queryBuilder.append("INSERT INTO provisionalPlms.weightmentDetails (tripId, weightType, weight, isToleranceFailed, entryTime, exitTime, toleranceNegative,")
                .append(" tolerancePositive, weighbridgeCode, weighridgeImg, toleranceApproverUser, toleranceActionDateTime) VALUES ('")
                .append(tripId).append("','").append(weightType).append("', '").append(weight).append("','").append(isToleranceFailed).append("','")
                .append(entryTime).append("','").append(exitTime).append("',");

    if (toleranceNegative == null) {
        queryBuilder.append("NULL");
    } else {
        queryBuilder.append(toleranceNegative);
    }

    queryBuilder.append(",");

    if (tolerancePositive == null) {
        queryBuilder.append("NULL");
    } else {
        queryBuilder.append(tolerancePositive);
    }

    queryBuilder.append(",'").append(weighbridgeCode).append("','").append(weighridgeImg).append("','")
                .append(toleranceApproverUser).append("',").append("NOW()").append(")");
}

String createWBquery = queryBuilder.toString();
kcontext.setVariable("weightQuery", createWBquery);
System.out.println("---create weightment query is :" + createWBquery);
]]></drools:script>
