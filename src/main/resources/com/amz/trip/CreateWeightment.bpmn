        updateWbDataquery = "UPDATE provisionalPlms.trip t JOIN vw_trip_stages ts ON t.tripid = ts.tripid SET t.mapPlantStageLocation = ts.Next_location, t.tareWeight = NOW(), t.tw = '" + weight + "', t.sequence_number = sequence_number+1 WHERE ts.tag_id = '" + tagId + "'";
